   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "9b29077b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "X original data [1 2 3]\n",
      "Y copy from X data [1 2 3]\n"
     ]
    }
   ],
   "source": [
    "X = np.array([1,2,3])\n",
    "Y = X.copy()\n",
    "print(\"X original data\", X)\n",
    "print(\"Y copy from X data\", Y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "b0fc074b",
   "metadata": {},
   "outputs": [
    {
